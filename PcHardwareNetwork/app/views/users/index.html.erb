<h1>All users</h1>

<div class="row">
    <div class="col-md-5">
        <ul class="list-group">
        <% @users.each do |user| %>
            <li class="list-group-item"><%= user.email %></li>
        <% end %>
        </ul>
    </div>
</div>
<br/>
<div class="digg_pagination">
  <%= will_paginate(@users) %>
</div>